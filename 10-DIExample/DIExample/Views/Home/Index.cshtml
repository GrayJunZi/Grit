@model IEnumerable<string>
@inject ICitiesService CitiesService
@{
    ViewBag.Title = "Home";
}

<h1>Inject ICitiesService</h1>

<ul>
    @foreach (var city in CitiesService.GetCities())
    {
        <li>@city</li>
    }
</ul>

<h1>Cities</h1>

<ul>
    @foreach (var city in Model)
    {
        <li>@city</li>
    }
</ul>

<h1>Instances</h1>

<ul>
    @foreach (var instanceId in ViewBag.Instances)
    {
        <li>@instanceId</li>
    }
</ul>